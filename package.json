{
  "name": "karma-benchmark",
  "version": "0.1.0",
  "description": "Example of use KarmaJS with Benchmark.js. I.e. start KarmaJS with multi config files. Additionally there are used some badges (shields).",
  "main": "index.js",
  "scripts": {
    "test": "npm run test_server_side && npm run test_client_side && npm run test_end_to_end",
    "test_client_side": "node karma_start_all_configs",
    "test_server_side": "mocha $(find spec/ ! -path '*.cs.js')",
    "test_end_to_end": "mocha -t 10000 end_to_end/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "karma": "1.1.1",
    "karma-benchmark": "^0.6.0",
    "karma-benchmark-reporter": "0.1.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "1.1.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "1.0.1",
    "mocha": "^3.1.0",
    "selenium-webdriver": "^3.0.0-beta-3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  }
}
